<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anje.kelvin.aconting">


    <application
        android:name="com.anje.kelvin.accontingapp.Classes.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Pakitini"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".Operacoes.Adicionar_despesaActivity"
            android:label="Adicionar Despesa"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Operacoes.Adicionar_deposito_Activity"
            android:label="Adicionar Deposito"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login"
            android:label="Login"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Operacoes.Venda_Activity"
            android:label="Vender"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Operacoes.Adicionar_item_venda_Activity" />
        <activity
            android:name=".Operacoes.RelatorioActivity"
            android:label="Relatorios"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Relatorios.RelatoriodeactividadesActivity" />
        <activity android:name=".Relatorios.RelatoriodeDespesas" />
        <activity
            android:name=".Relatorios.Relatorio_de_Despesas_Activity"
            android:label="Relatorio de Despesas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Operacoes.RegistrarActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".Login_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.anje.kelvin.aconting.Login_Activity" />
        </activity>
        <activity
            android:name=".Relatorios.Relatorio_de_renda_Activity"
            android:label="Relatorio de Renda"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Operacoes.Estoque_Activity"
            android:label="Gerir Estoque"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.anje.kelvin.aconting.MainActivity" />
        </activity>
        <activity
            android:name=".Operacoes.TransicoesActivity"
            android:label="Transacoes"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.anje.kelvin.aconting.MainActivity" />
        </activity>
        <activity
            android:name=".Operacoes.Itens_venda_Activity"
            android:label="Itens Venda"
            android:parentActivityName=".Operacoes.Venda_Activity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.anje.kelvin.aconting.Operacoes.Venda_Activity" />
        </activity>
        <activity
            android:name=".Relatorio_de_Transicoes_Activity"
            android:label="@string/title_activity_relatorio_de__transicoes_"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>